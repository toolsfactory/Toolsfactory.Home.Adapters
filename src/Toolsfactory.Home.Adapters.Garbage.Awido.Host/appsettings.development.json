{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\abfall.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1048576
        }
      }
    ]
  },
  "services": {
    "garbage": {
      "StartupDelaySeconds": 5,
      "UpdateDelaySeconds": 30
    }
  }
}