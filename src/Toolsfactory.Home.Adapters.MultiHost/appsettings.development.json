{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\multihost.log"
        }
      }
    ]
  },

  "services": {
    "garbage": {
      "StartupDelaySeconds": 5,
      "UpdateDelaySeconds": 30
    },
    "gasprices": {
      "StartupDelaySeconds": 5,
      "UpdateDelaySeconds": 300
    },
    "heating": {
      "StartupDelaySeconds": 2
    },
    "powermeter": {
      "SerialPort": "COM3"
    }
  }
}