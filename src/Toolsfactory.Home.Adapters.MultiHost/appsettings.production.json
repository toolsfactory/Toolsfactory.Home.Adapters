{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/var/log/ohadapters/multihost.log"
        }
      }
    ]
  },
  "services": {
    "gasprices": {
      "StartupDelaySeconds": 15,
      "UpdateDelaySeconds": 900,
      "TankerKoenig": {
        "ApiKey": ""
      }
    },
    "garbage": {
      "StartupDelaySeconds": 5,
      "UpdateDelaySeconds": 30
    },
    "powermeter": {
      "SerialPort": "/dev/ttyUSB0"
    }
  }
}